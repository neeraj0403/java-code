package Assignment_hackerblocks;

import java.util.Scanner;

public class chessboard_problem {
static int ct=0;
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int board[][]=new int[n][n];
		chess(board,0,0,n,n,"");
		System.out.println("\n"+ct);

	}
public static void chess(int board[][],int cc,int cr,int ec,int er,String ans)
{  if(cc==ec&&cr==er)
{
	System.out.print(ans+" ");
	ct++;
	return;
}
	if(cc>board[0].length||cr>board.length)
		return;
	
	
	chess(board,cc+2,cr+1,ec,er,ans+"{"+cc+"-"+cr+"}"+"K");
	
	chess(board,cc+1,cr+2,ec,er,ans+"{"+cc+"-"+cr+"}"+"K");
	
	for(int i=1;cc+i<board[0].length;i++)
		chess(board,cc+1,cr,ec,er,ans+"{"+cc+"-"+cr+"}"+"R");
	for(int i=1;cr+i<board.length;i++)
		chess(board,cc,cr+1,ec,er,ans+"{"+cc+"-"+cr+"}"+"R");
	
	if(cc==cr||cc+cr==er)
	{
		chess(board,cc+1,cr+1,ec,er,ans+"{"+cc+"-"+cr+"}"+"B");
	}
	
		
	
	
	
}

	
	
	
}

