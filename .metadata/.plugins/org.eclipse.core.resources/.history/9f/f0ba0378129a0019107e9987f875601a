package pattern;

import java.util.Scanner;

public class simple {

	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int i=1;
		
		while(n>0)
		{
			
			for(int j=0;j<i;j++)
			{
				System.out.print(" "+"*"+" ");
			}
			i++;
			n--;
			System.out.println();
			
		}
		

	}

}
