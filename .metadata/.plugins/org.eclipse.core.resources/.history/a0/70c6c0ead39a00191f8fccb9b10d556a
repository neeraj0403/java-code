package pattern;

import java.util.Scanner;

public class simple {

	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int nst=1;
		
		while(n>0)
		{
			
			for(int cst=0;cst<nst;cst++)
			{
				System.out.print("  *");
			}
			nst++;
			n--;
			System.out.println();
			
		}
		

	}

}
